{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=YourDatabase;User Id=user;Password=password;TrustServerCertificate=true;Encrypt=false;Pooling=false;MultipleActiveResultSets=true"
  },
  "Database": {
    "Provider": "SqlServer",
    "ConnectionStringName": "DefaultConnection",
    "CommandTimeoutSeconds": 30,
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": false,
    "UseDbContextPool": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    }
  },
  "AllowedHosts": "*",
  "RabbitMqSettings": {
    "Host": "localhost",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "custom-topic",
    "RecreateQueues": true,
    "TopicConsumerQueueName": "subscriptions.consumer.queue"
  }
}
